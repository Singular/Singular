blc ShiftDVec::enterOnePair 'enterL'
commands
  silent
  if $enter_cnt < 8 || 1
    printf "\nin enterOnePair before enterL. Counter: %d\n", $enter_cnt
    call (void)fflush(0x0)
    pll Lp
    set $enter_cnt = $enter_cnt + 1
    continue
  end
end

