blc ShiftDVec::bba 'if (hilb!=NULL)'
commands
  silent
  printf "\n\nstrat->S after enterS %03d:\n", $strat_cnt
  pss
  set $strat_cnt = $strat_cnt + 1
  if $strat_cnt < 20
    continue
  end
end

