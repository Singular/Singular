source gdb_get_line.py

define disable_output
   set logging redirect on
   set logging file /dev/null
   set logging on
end

define enable_output
   set logging off
end

break ShiftDVec::bba
commands
  set $spl_cnt=0
#  watch strat->P if 1 > 2
  continue
end

break ShiftDVec::bba:445
commands
  silent
  printf "\n\nNew Element from strat->L\n"
  printf "  strat->P.p (ptr): %p\n", strat->P.p
  printf "  strat->P.p: "
  pct strat->P.p
  printf "\n"
  continue
end

break ShiftDVec::enterpairs
commands
  silent
  printf "\n\nAt start of enterpairs\n"
  printf "  H->p (ptr): %p\n", H->p
  printf "  H->p: "
  pct H->p
  printf "\n"
  continue
end

