static proc deletecol(matrix M,int l)
{
  if (ncols(M)==1)
    {
      return(M);
    }
  int s=ncols(M);
  if (l==1)
    {
      M=submat(M,(1..nrows(M)),(2..ncols(M)));
      return(M);
    }
  if (l==s)
    {
      M=submat(M,(1..nrows(M)),(1..(ncols(M)-1)));
      return(M);
    }
  intvec v=(1..(l-1)),((l+1)..s);
  M=submat(M,(1..nrows(M)),v);
  return(M);
}

