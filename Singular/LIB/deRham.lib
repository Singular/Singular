static proc VdDegnhv(matrix M,int d,intvec v,list #)
{
  /* As the procedure VdDeg, but the basering is the nth Weyl algebra
     with a commutative variable nhv*/
  int i,j,etoint;
  int n=nvars(basering) div 2;
  intvec  e;
  int etoint;
  list findmax;
  int c=ncols(M);
