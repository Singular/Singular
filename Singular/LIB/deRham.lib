static proc sublist(list L,int m,int n)
{
  list out;
  int i; int j;
  int count;
  for (i=m; i<=n; i++)
    {
      out[size(out)+1]=list();
      for (j=1; j<=size(L[i]); j++)
        {
          count=count+1;
          out[size(out)][j]=list(L[i][j],count);
        }
    }
  list o=list(out,count);
  return(o);
}

