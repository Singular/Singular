static proc nDeg (matrix M,intvec m)
{/*we compute an intvec n such that n[i]=max(deg(M[i,j])+m[j]|M[i,j]!=0) (where deg
   stands for the total degree) if (M[i,j]!=0 for some j) and n[i]=0 else*/
  int i; int j;
  intvec n;
  list L;
  for (i=1; i<=nrows(M); i++)
    {
      L=list();
      for (j=1; j<=ncols(M); j++)
        {
          if (M[i,j]!=0)
            {
              L=insert(L,deg(M[i,j])+m[j]);
            }
        }
      if (size(L)==0)
        {
          n[i]=0;
        }
      else
        {
          n[i]=Max(L);
        }
    }
  return(n);
}

