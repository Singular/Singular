static proc mHom(poly f)
{/*for globalBFunOT*/
  poly g;
  poly l;
  poly add;
  intvec e;
  list minint;
  list remf;
  int i;
  int j;
  int n=nvars(basering) div 4;
  if (f==0)
    {
      return(f);
    }
  while (f!=0)
    {
      l=lead(f);
      e=leadexp(l);
      remf[size(remf)+1]=list();
      remf[size(remf)][1]=l;
      for (i=1; i<=n; i++)
        {
          remf[size(remf)][i+1]=-e[2*n+i]+e[3*n+i];
          if (size(minint)<i)
            {
              minint[i]=list();
            }
          minint[i][size(minint[i])+1]=-e[2*n+i]+e[3*n+i];
        }
      f=f-l;
    }
  for (i=1; i<=n; i++)
    {
      minint[i]=Min(minint[i]);
    }
  for (i=1; i<=size(remf); i++)
    {
      add=remf[i][1];
      for (j=1; j<=n; j++)
        {
          add=v(j)^(remf[i][j+1]-minint[j])*add;
        }
      g=g+add;
    }
  return (g);
}

