static proc permuteVar(list L,int n)
{/*for globalBFunOT*/
  if (typeof(L[1])=="intvec")
    {
      intvec v=L[1];
    }
  else
    {
      intvec v=(1:L[1]),(0:L[1]);
    }
  int i;int k; int indi=0;
  int j;
  int s=size(v);
  poly e;
  intvec fore;
  for (i=2; i<=size(v); i=i+2)
    {

      if (v[i]!=0)
        {
           j=i+1;
          while (v[j]!=0)
            {
              j=j+1;
            }
          v[i]=0;
          v[j]=1;
          fore=0;
          indi=0;
          for (k=1; k<=size(v); k++)
            {
              if (k!=i and k!=j)
                {
                  if (indi==0)
                    {
                      indi=1;
                      fore[1]=v[k];
                    }
                  else
                    {
                      fore[size(fore)+1]=v[k];
                    }
                }
            }
          e=e-(j-i)*permutevar(list(fore),n);
        }
    }
  e=e+s(n)^(size(v) div 2);
  return (e);
}

