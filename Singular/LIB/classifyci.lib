////////////////////////////////////////////////////////////////////////////////
static  proc typejet2(poly g1,poly g2)
"USAGE:  typejet2(g1,g2);  g1,g2 are polynomials
ASSUME: g1,g2 are homogeneous polynomials of degree 2
PURPOSE: Check whether (g1,g2) is a quadratic form in the list of Guisti or not
RETURN: string  type for the quadratic forms appearing in Guist's list
@*       or not simple
{
  def R=basering;
  ideal I=(g1,g2);
  def S=absPrimdecGTZ(I);
  setring S;
  list L,T;
  int e,i,j;
  intvec a,a1;
  L=primary_decomp;
  for(j=1;j<=size(L);j++)
  {
    if(dim(std(L[j][1]))!=2)
    {
      return("not simple");
    }
  }
  T=absolute_primes;
  for(i=1;i<=size(T);i++)
  {
    e=e+T[i][2];
  }
  if(e==4)
  {
    setring R;
    return("type1");
  }
  if(e==3)
  {
    setring R;
    return("type2");
  }
  if(e==2)
  {
    ideal J=std(L[1][1]);
    ideal J1=std(L[2][1]);
    a=hilbPoly(J);
    a1=hilbPoly(J1);
    if((a[2]==2)&&(a1[2]==2))
    {
      setring R;
      return("type3");
    }
    if(((a[2]==3)&&(a1[2]==1))||((a[2]==1)&&(a1[2]==3)))                      //||(a[2]==1)&&(a1[2]==3))
    {
      setring R;
      return("type4");
    }
  }
  if(e==1)
  {
    setring R;             // I lies in R and zero in S1
    ideal JJ=radical(I);
    JJ=JJ^3;
    ideal JJJ=reduce(JJ,std(I));
    if(size(JJJ)==0)
    {
      return("type6");
    }
    if(size(JJJ)!=0)
    {
      return("type5");
    }
  }
  setring R;
  return("not simple");
}
