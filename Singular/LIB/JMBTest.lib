{ "EXAMPLE:"; echo = 2;
 ring r=0, (x,y,z), rp;
 jmp r1;
 r1.h=z^3;
 r1.t=poly(0);
 jmp r2;
 r2.h=z^2*y;
 r2.t=poly(0);
 jmp r3;
