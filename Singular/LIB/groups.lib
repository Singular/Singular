static proc insResult(list r,#)
{
   if(size(#)==0){return(r);}
   if(size(r)==0){r[1]=1;}
   int i,j;
   for(i=1;i<=size(#);i++)
   {
      j=1;
      while((#[i]>r[j])&&(j<size(r))){j++;}
      if(#[i]>r[j]){r=insert(r,#[i],j);}
      if(#[i]<r[j]){r=insert(r,#[i],j-1);}
   }
   return(r);
}
example
{ "EXAMPLE:"; echo = 2;
