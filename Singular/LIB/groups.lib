   fac=factorize(I[1]);

   while((size(fac[1])==2)&&(i<size(I)))
   {
      i++;
      fac=factorize(I[i]);
   }
   if(size(fac[1])>2)
   {
      w=squarefreeP(number(fac[1][1]));
      n=w[1];
      qr=insResult(qr,w[2]);
      for(j=2;j<=size(fac[1]);j++)
