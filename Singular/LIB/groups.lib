static proc shortid_L (ideal id,int n)
{
  ideal Lc;
  intvec v;
  int ii;
  for(ii= 1; ii<=ncols(id); ii++)
  {
   if (size(id[ii])<=n and id[ii]!=0)
   {
       Lc = Lc,id[ii];
       v=v,ii;
   }
  }
  Lc = simplify(Lc,2);
  list L = Lc,v;
  return(Lc);
}


