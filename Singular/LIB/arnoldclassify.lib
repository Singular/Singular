        f.Restrictions = \" (k>1)&&(r>=0)&&(deg(a)<=(k-2))&&(jet(a,0)!=0)&&
        (jet(b,0)!=0)&&(deg(b)<=(2*k+r-2))\";";
        write(l, "Z[k,12k+6r+1]", s);

    //* Z[1,6r+13]
        s = "singseries f;
        f.Series = \"Z[1,6r+13]\";
        f.Modality = \"1+r\";
        f.Corank = \"2\";
