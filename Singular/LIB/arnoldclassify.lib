        f.Restrictions = \"(k>1)&&(r>0)&&(deg(a)<=(k-2))&&(jet(a,0)^2!=4)&&
        (jet(b,0)!=0)&&(deg(b)<=(2*k-2))\";";
      write(l, "X[k,r]", s);

    //* X[1,r]
