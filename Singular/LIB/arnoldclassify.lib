        f.NormalForm = \"y*(x^3 + x*y^(2*r+3) +b(y)* y^(3*r+5))\";
        f.SpecialForm = \"y*(x^3 + x*y^(2*r+3) +y^(3*r+5))\";
        f.Restrictions = \"(r>=0)&&(deg(b)<=(r))\";";
        write(l, "Z[1,6r+12]", s);


    //* W[k,r]
        s = "singseries f;
        f.Series = \"W[k,r]\";
        f.Modality = \"3*k-1\";
        f.Corank = \"2\";
