            f.SpecialForm = \"y*(x^3 + x*y^(2+2*r+1) + y^(3+3*r+1))\";
            f.Restrictions = \"(r>=0)&&(deg(b)<=(r))\";";
            write(l, "Z[1,6r+11]", s);

    //* Z[k,12k+6r+1]
        s = "singseries f;
        f.Series = \"Z[k,12k+6r+1]\";
        f.Modality = \"3*k+r-2\";
        f.Corank = \"2\";
        f.MilnorNumber = \"12*k+6r+1\";
        f.MilnorCode = \"1,1,2k-1,2k-1+j,2k+3+6*r-2*j\";
        f.NormalForm = \"(x + a(y)*y^k)*(x^3 + b(y)*x*y^(2*k+2*r+2) +
