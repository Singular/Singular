write(fname,"If an algebraic extension should be displayed together with
its minimal polynomial the optional parameter \\verb|mipo| has to be used.");

write(fname,
bv +
"> texring(fname,ralg,\"mipo\");" +
ev
);

texring(fname, ralg,"mipo");
