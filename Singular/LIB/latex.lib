write(fname, bv +
"> poly g=-2*x1+x4-1;  g;" +nl +
"> texpoly(fname,g);" +
ev);

  texpoly(fname,g);

write(fname,"\\Line","%-----------------------------------------------------");

// -3-------- a ring in char 0, indexed varnames and local ordering ----------
